        "@stencil/sass": "^1.0.1",
        "@types/jest": "24.0.15",
        "@types/lodash": "^4.14.134",
        "@types/node": "^12.0.10",
        "@types/numeral": "0.0.25",
        "@types/puppeteer": "1.12.4",
        "jest": "24.8.0",
