{
    "name": "ketchup-lite",
    "version": "2.0.0",
    "keywords": [
        "ketchup lite",
        "web component",
        "library",
        "Stencil",
        "pwa",
        "custom elements",
        "components"
    ],
    "description": "Web components library",
    "license": "Apache-2.0",
    "main": "./dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "unpkg": "dist/ionic.js",
    "types": "dist/types/components.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rimraf target && stencil build --docs",
        "dev": "rimraf target && stencil build --dev --watch --serve",
        "test": "cypress run",
        "test:open": "cypress open"
    },
    "dependencies": {
        "dayjs": "^1.11.11",
        "echarts": "5.5.1",
        "html2canvas": "^1.4.1",
        "numeral": "^2.0.6",
        "prismjs": "^1.29.0"
    },
    "devDependencies": {
        "@stencil/core": "v4.18.1",
        "@stencil/react-output-target": "0.5.3",
        "@stencil/sass": "^3.0.12",
        "@types/cypress": "^1.1.3",
        "@types/node": "^20.2.5",
        "@types/numeral": "^2.0.5",
        "@types/prismjs": "^1.26.4",
        "cypress": "^13.13.0",
        "prettier-eslint": "16.3.0",
        "puppeteer": "^22.9.0",
        "rimraf": "^6.0.0",
        "typescript": "^5.5.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lucafoscili/ketchup-lite.git"
    },
    "installConfig": {
        "hoistingLimits": "workspaces"
    }
}
