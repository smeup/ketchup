{
  "name": "@sme.up/ketchup-showcase",
  "version": "2.0.0",
  "keywords": [
    "smeup",
    "KetchUP showcase",
    "pwa",
    "custom elements",
    "components",
    "Vue.js"
  ],
  "description": "Ketchup Web Components library by smeup",
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:bundle": "rimraf dist && vue-cli-service build --target lib --name vueup ./src/components/index.js",
    "build:label": "rimraf dist && vue-cli-service build --target lib --name SmeupLabel src/components/SmeupLabel.vue",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "vue-cli-service serve",
    "test:e2e:open": "start-test 4000 cy:open",
    "test:e2e:run": "start-test 4000 cy:run",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@sme.up/ketchup": "^6.9.0-SNAPSHOT",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.17",
    "vue-cli-plugin-webpack-bundle-analyzer": "^4.0.0",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/jest": "^26.0.21",
    "@types/jest-cli": "^24.3.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "cypress": "^4.8.0",
    "cypress-shadow-dom": "^1.0.0",
    "jest-html-reporter": "^3.4.1",
    "jest-junit": "^12.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.46.0",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.10",
    "ts-jest": "^27.0.3",
    "typescript": "4.2.3",
    "vue-template-compiler": "^2.5.17"
  }
}
